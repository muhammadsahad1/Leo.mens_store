<script>
  
  fetch('/getWishlist')
 .then(response => response.json())
 .then(wishlist => {
    const productId = document.querySelector('.wishlist-icon').getAttribute('data-product-id');
    const isInWishlist = wishlist.includes(productId);

    // Update the wishlist icon based on whether the product is in the wishlist
    if (isInWishlist) {
      // Product is in wishlist, show view wishlist icon
      document.querySelector('.wishlist-icon').style.display = 'none';
      document.querySelector('.wishlist-icon2').style.display = 'block';
    }
 });

// Attach event listener to wishlist icon
document.querySelectorAll(".wishlist-icon").forEach((icon) => {
 icon.addEventListener("click", (event) => {
    event.preventDefault();
    const productId = icon.getAttribute("data-product-id");
    const userId = document.getElementById('userId').value;

    if (!userId) {
      // User not logged in, handle accordingly (e.g., show login prompt)
      Swal.fire({
        icon: 'error',
        title: 'Must login to add wishlist',
        timer: 1700
      });
      return;
    }

    // Send request to add product to wishlist
    fetch("/addWishlist", {
      method: "POST",
      headers: {
        "Content-Type": "application/json",
      },
      body: JSON.stringify({ productId, userId }),
    }).then((response) => {
      if (response.ok) {
        // Update UI after adding to wishlist
        icon.style.display = 'none'; // Hide the add to wishlist icon
        const viewWishlistIcon = document.querySelector('.wishlist-icon2');
        viewWishlistIcon.style.display = 'block'; // Show the view wishlist icon
        viewWishlistIcon.href = '/wishlist'; // Ensure the link is correct
      }
    });
 });
});

// Attach event listener to view wishlist icon
document.querySelector('.wishlist-icon2').addEventListener('click', function(event) {
 event.preventDefault();
 // Redirect to wishlist page or handle as needed
});
</script>

    
